{
  "dockerImage": "openjdk:8-jre",
  "modules": {
    "com.fasterxml.jackson.core": {
      "jackson": {
        "exactName": false,
        "version": "2.9.8"
      },
      "jackson-annotations": {
        "overrideIsEnough": false,
        "version": "2.9.8"
      },
      "jackson-core": {
        "overrideIsEnough": false,
        "version": "2.9.8"
      },
      "jackson-databind": {
        "overrideIsEnough": false,
        "version": "2.9.8"
      }
    },
    "com.fasterxml.jackson.datatype": {
      "jackson-datatype-jdk8": {
        "overrideIsEnough": false,
        "version": "2.9.8"
      },
      "jackson-datatype-jsr310": {
        "overrideIsEnough": false,
        "version": "2.9.8"
      }
    },
    "com.fasterxml.jackson.module": {
      "jackson-module-scala_2.11": {
        "overrideIsEnough": false,
        "version": "2.9.8"
      }
    },
    "com.google.guava": {
      "guava": {
        "version": "24.1.1-android"
      }
    },
    "com.typesafe.play": {
      "play-": {
        "exactName": false,
        "excludeName": [
          "play-json",
          "play-functional",
          "play-ws-",
          "play-ahc-ws-"
        ],
        "version": "2.6.20"
      },
      "play-ahc-ws-": {
        "exactName": false,
        "version": "1.1.10"
      },
      "play-functional_2.12": {
        "version": "2.6.10"
      },
      "play-json_2.12": {
        "dependenciesToRemove": [
          "com.fasterxml.jackson.core | jackson-annotations",
          "com.fasterxml.jackson.core | jackson-core",
          "com.fasterxml.jackson.core | jackson-databind",
          "com.fasterxml.jackson.datatype | jackson-datatype-jdk8",
          "com.fasterxml.jackson.datatype | jackson-datatype-jsr310"
        ],
        "version": "2.6.10"
      },
      "play-ws-": {
        "exactName": false,
        "version": "1.1.10"
      }
    },
    "commons-codec": {
      "commons-codec": {
        "version": "1.10"
      }
    },
    "commons-beanutils": {
      "commons-beanutils": {
        "overrideIsEnough": false,
        "version": "1.9.3"
      },
      "commons-beanutils-core": {
        "overrideIsEnough": false,
        "version": "None"
      }
    },
    "io.netty": {
      "netty-all": {
        "overrideIsEnough": false,
        "version": "4.1.31.Final"
      }
    },
    "io.spray": {
      "spray-json": {
        "forbidden": "You should not include spray-json. If you need a json parser, please use 'circe', you can look at it at https://github.com/circe/circe .",
        "version": "None"
      }
    },
    "janstenpickle.vault": {
      "vault-": {
        "exactName": false,
        "version": "None"
      }
    },
    "jline": {
      "jline": {
        "overrideIsEnough": false,
        "version": "2.14.6"
      }
    },
    "org.apache.commons": {
      "commons-compress": {
        "version": "1.18"
      }
    },
    "org.apache.hadoop": {
      "hadoop-": {
        "exactName": false,
        "forbidden": "You should not include hadoop in your build.sbt. Spark is fetching it for you and the machine the job will run on will have its own version.",
        "version": "None"
      },
      "hadoop-hdfs": {
        "version": "2.7.5"
      },
      "hadoop-mapreduce-client-core": {
        "version": "2.7.5"
      },
      "hadoop-yarn-common": {
        "version": "2.7.5"
      },
      "hadoop-yarn-server-nodemanager": {
        "version": "2.7.5"
      }
    },
    "org.apache.spark": {
      "spark-": {
        "dependenciesToRemove": [
          "com.fasterxml.jackson.core | jackson-annotations",
          "com.fasterxml.jackson.core | jackson-core",
          "com.fasterxml.jackson.core | jackson-databind",
          "com.fasterxml.jackson.module | jackson-module-scala_2.11",
          "com.google.guava | guava",
          "commons-beanutils | commons-beanutils",
          "commons-beanutils | commons-beanutils-core",
          "commons-codec | commons-codec",
          "io.netty | netty-all",
          "org.apache.commons | commons-compress",
          "org.apache.hadoop | hadoop-hdfs",
          "org.apache.hadoop | hadoop-mapreduce-client-core",
          "org.apache.hadoop | hadoop-yarn-common",
          "org.apache.hadoop | hadoop-yarn-server-nodemanager",
          "org.bouncycastle | bcprov-jdk15on",
          "org.codehaus.jackson | jackson-mapper-asl",
          "org.glassfish.jersey.core | jersey-common",
          "org.glassfish.jersey.media | jersey-media-jaxb"
        ],
        "exactName": false,
        "shouldBeProvided": true,
        "version": "2.3.1"
      }
    },
    "org.bouncycastle": {
      "bcprov-jdk15on": {
        "version": "1.60"
      }
    },
    "org.codehaus.jackson": {
      "jackson-mapper-asl": {
        "overrideIsEnough": false,
        "version": "None"
      }
    },
    "org.glassfish.jersey.core": {
      "jersey-common": {
        "version": "2.27"
      }
    },
    "org.glassfish.jersey.media": {
      "jersey-media-jaxb": {
        "version": "2.27"
      }
    },
    "org.pac4j": {
      "pac4j-": {
        "exactName": false,
        "version": "3.3.0"
      },
      "play-pac4j": {
        "needDoublePercent": true,
        "shouldDownload": false,
        "version": "6.0.0"
      }
    },
    "org.rogach": {
      "scallop": {
        "needDoublePercent": true,
        "version": "3.3.1"
      }
    },
    "org.scalatest": {
      "scalatest": {
        "needDoublePercent": true,
        "version": "3.0.5"
      }
    }
  },
  "sbtVersion": "1.2.6",
  "scalaCFlags": [
    "-feature",
    "-Yrangepos",
    "-Ywarn-dead-code",
    "-Ywarn-inaccessible",
    "-Ywarn-unused",
    "-Ywarn-unused-import",
    "-Ywarn-value-discard",
    "-deprecation",
    "-encoding",
    "utf8"
  ],
  "scalaVersions": [
    "2.12.7",
    "2.11.12",
    "2.13.1"
  ]
}
